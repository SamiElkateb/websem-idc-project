recipes:BananaCreamPie :hasIngredient [ a :Ingredient ;
            :food food:Cornstarch ;
            :name "cornstarch" ;
            :quantity "3" ;
            :unit measurements:Tablespoon ],
        [ a :Ingredient ;
            :food food:Butter ;
            :name "margarine or butter" ;
            :quantity "2" ;
            :unit measurements:Tablespoon ],
        [ a :Ingredient ;
