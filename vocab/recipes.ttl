            :name "vanilla" ;
            :quantity "1" ;
            :unit measurements:Teaspoon ],
        [ a :Ingredient ;
            :food food:Water ;
            :name "water" ;
            :quantity "1 2/3" ;
            :unit measurements:Cup ],
        [ a :Ingredient ;
