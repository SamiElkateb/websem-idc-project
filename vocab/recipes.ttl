            :name "sugar" ;
            :quantity "3/4" ;
            :unit measurements:Cup ],
        [ a :Ingredient ;
            :food <http://project-kitchenchef.fr/food/data#Pumpkin> ;
            :name "pumpkin" ;
            :quantity "1" ;
            :unit measurements:Cup ],
