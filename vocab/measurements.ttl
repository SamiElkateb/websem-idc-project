@prefix : <http://project-kitchenchef.fr/schema#> .

@prefix measurements: <http://project-kitchenchef.fr/measurements/data#> .

@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .



measurements:Tablespoon a :VolumeMeasurementUnit,
        skos:Concept ;
        :hasUnitConversion [ a :UnitConversion ;
          :unit measurements:Teaspoon ;
          :conversionRatio 3.0
        ] .

measurements:Teaspoon a :VolumeMeasurementUnit,
        skos:Concept .

measurements:Liter a :VolumeMeasurementUnit,
        skos:Concept .

measurements:centiLiter a :VolumeMeasurementUnit,
        skos:Concept ;
        :hasUnitConversion [ a :UnitConversion ;
          :unit measurements:Liter ;
          :conversionRatio 0.01
        ] .

measurements:milliLiter a :VolumeMeasurementUnit,
        skos:Concept ;
        :hasUnitConversion [ a :UnitConversion ;
          :unit measurements:Liter ;
          :conversionRatio 0.001
        ] .

measurements:Cup a :VolumeMeasurementUnit,
        skos:Concept ;
        :hasUnitConversion [ a :UnitConversion ;
          :unit measurements:Liter ;
          :conversionRatio 0.2366
        ] .

measurements:Package a :MeasurementUnit,
        skos:Concept .

measurements:Slice a :MeasurementUnit,
        skos:Concept .

measurements:Pinch a :MeasurementUnit,
                     skos:Concept .

measurements:Slice a :MeasurementUnit,
                     skos:Concept .

measurements:VolumicOunce a :VolumeMeasurementUnit,
        skos:Concept ;
        :hasUnitConversion [ a :UnitConversion ;
          :unit measurements:Liter ;
          :conversionRatio 0.0296
        ] .

measurements:Quart a :VolumeMeasurementUnit,
        skos:Concept ;
        :hasUnitConversion [ a :UnitConversion ;
          :unit measurements:Liter ;
          :conversionRatio 0.8798789
        ] .

measurements:Pound a :WeightMeasurementUnit,
        skos:Concept ;
        :hasUnitConversion [ a :UnitConversion ;
          :unit measurements:Kilogram ;
          :conversionRatio 0.4536
        ] .

measurements:WeightOunce a :WeightMeasurementUnit,
        skos:Concept ;
        :hasUnitConversion [ a :UnitConversion ;
          :unit measurements:Kilogram ;
          :conversionRatio 0.02835
        ] .

measurements:Gram a :WeightMeasurementUnit,
        skos:Concept ;
        :hasUnitConversion [ a :UnitConversion ;
          :unit measurements:Kilogram ;
          :conversionRatio 1000.0
        ] .

measurements:Kilogram a :WeightMeasurementUnit,
        skos:Concept .

