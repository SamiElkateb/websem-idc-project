@prefix : <http://project-kitchenchef.fr/schema#> .

@prefix measurements: <http://project-kitchenchef.fr/measurements/data#> .

@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .


measurements:Tablespoon a :VolumeMesurementUnit,
        skos:Concept ;
        :hasUnitConversion [
          :unit measurements:Teaspoon ;
          :conversionRatio 3
        ] .

measurements:Teaspoon a :VolumeMesurementUnit,
        skos:Concept .

measurements:Liter a :VolumeMesurementUnit,
        skos:Concept .

measurements:centiLiter a :VolumeMesurementUnit,
        skos:Concept ;
        :hasUnitConversion [
          :unit measurements:Liter ;
          :conversionRatio 0.01
        ] .

measurements:milliLiter a :VolumeMesurementUnit,
        skos:Concept ;
        :hasUnitConversion [
          :unit measurements:Liter ;
          :conversionRatio 0.001
        ] .

measurements:Cup a :VolumeMesurementUnit,
        skos:Concept ;
        :hasUnitConversion [
          :unit measurements:Liter ;
          :conversionRatio 0.2366
        ] .

measurements:Package a :VolumeMesurementUnit,
        skos:Concept .

measurements:VolumicOunce a :MesurementUnit,
        skos:Concept ;
        :hasUnitConversion [
          :unit measurements:Liter ;
          :conversionRatio 0.0296
        ] .

measurements:Pinch a :MesurementUnit,
        skos:Concept .

measurements:Slice a :MesurementUnit,
        skos:Concept .

measurements:Pound a :WeightMesurementUnit,
        skos:Concept ;
        :hasUnitConversion [
          :unit measurements:Kilogram ;
          :conversionRatio 0.4536
        ] .

measurements:WeightOunce a :WeightMesurementUnit,
        skos:Concept ;
        :hasUnitConversion [
          :unit measurements:Kilogram ;
          :conversionRatio 0.02835
        ] .

measurements:Gram a :WeightMesurementUnit,
        skos:Concept ;
        :hasUnitConversion [
          :unit measurements:Kilogram ;
          :conversionRatio 1000
        ] .

measurements:Kilogram a :WeightMesurementUnit,
        skos:Concept .

