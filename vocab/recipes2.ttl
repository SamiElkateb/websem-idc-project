recipes:BlueberryCoffeeCake a :Recipe ;
    :hasIngredient [ a :Ingredient ;
            :food food:Sugar ;
            :name "sugar" ;
            :quantity 1.5 ;
            :unit measurements:Cup ],
        [ a :Ingredient ;
            :food food:Butter ;
            :name "butter" ;
            :quantity 0.25 ;
            :unit measurements:Cup ],
        [ a :Ingredient ;
            :food food:Eggs ;
            :name "eggs" ;
            :quantity 3.0 ],
        [ a :Ingredient ;
            :food food:LemonJuice ;
            :name "lemon juice" ;
            :quantity 1.0 ;
            :unit measurements:Tablespoon ],
        [ a :Ingredient ;
            :food food:Vanilla ;
            :name "vanilla" ;
            :quantity 3.0 ;
            :unit measurements:Teaspoon ],
        [ a :Ingredient ;
            :food food:Flour ;
            :name "flour" ;
            :quantity 4.0 ;
            :unit measurements:Cup ],
        [ a :Ingredient ;
            :food food:BakingPowder ;
            :name "baking powder" ;
            :quantity 1.0 ;
            :unit measurements:Tablespoon ],
        [ a :Ingredient ;
            :food food:Milk ;
            :name "milk" ;
            :quantity 1.0 ;
            :unit measurements:Cup ],
        [ a :Ingredient ;
            :food food:Blueberries ;
            :name "blueberries" ;
            :quantity 3.0 ;
            :unit measurements:Cup ],
        [ a :Ingredient ;
            :food food:CreamCheese ;
            :name "cream cheese" ;
            :quantity "12" ;
            :unit measurements:WeightOunce ] ;
    :instructions """Mix butter, 1 cup sugar, flour, 2 eggs, 2 tsp vanilla, baking powder, and milk to make cake batter. Blend easily.  Add 3 cups of blueberries.  Put 2 1/2 cups of batter in pan.
Mix cream cheese, 1/2 cup sugar, 1 egg, lemon juice and 1 tsp vanilla to make filling.  Put filling on top of batter.  Top with remaining batter.
Make streusel topping from flour, butter, brown sugar, ad cinnamon.  Sprinkle on top of cake.
Bake at 375 degrees for 1 hour and 5 minutes.  Check after 50 minutes.""" ;
    :name "Blueberry Coffee Cake" ;
    :recipeCategory "Cakes" .

